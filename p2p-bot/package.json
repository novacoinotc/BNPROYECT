{
  "name": "binance-p2p-bot",
  "version": "1.0.0",
  "description": "Bot autónomo de P2P para Binance con auto-ajuste de precios y verificación automática de pagos",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test:api": "tsx src/test-api.ts",
    "discover": "tsx src/discover-endpoints.ts",
    "discover:extended": "tsx src/discover-endpoints-extended.ts",
    "discover:prod": "node dist/discover-endpoints.js",
    "dashboard": "cd dashboard && npm run dev",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "test": "vitest",
    "lint": "eslint src --ext .ts"
  },
  "keywords": [
    "binance",
    "p2p",
    "trading",
    "bot",
    "crypto"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "axios": "^1.6.7",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "ioredis": "^5.3.2",
    "node-cron": "^3.0.3",
    "pg": "^8.16.3",
    "pino": "^8.19.0",
    "pino-pretty": "^10.3.1",
    "tesseract.js": "^5.0.5",
    "ws": "^8.16.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@types/node-cron": "^3.0.11",
    "@types/pg": "^8.16.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "eslint": "^8.57.0",
    "prisma": "^5.9.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vitest": "^1.3.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
